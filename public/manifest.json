{
  "manifest_version": 2,
  "name": "Dareiu",
  "version": "0.1.0",
  "description": "誰が言ったかじゃなく何を言ったかでTwitterをみろよ",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/home"],
      "run_at": "document_idle",
      "css": ["styles.css"]
    }
  ]
}
